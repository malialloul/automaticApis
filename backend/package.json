{
  "name": "automatic-apis-backend",
  "version": "1.0.0",
  "description": "Backend service for automatic REST API generation from PostgreSQL schema",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "node test.js"
  },
  "keywords": ["api", "postgresql", "swagger", "openapi", "rest"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "swagger-ui-express": "^5.0.0",
    "morgan": "^1.10.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.7",
    "mongodb": "^5.7.0",
    "mssql": "^9.1.0",
    "oracledb": "^5.3.0",
    "node-sql-parser": "^4.5.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
